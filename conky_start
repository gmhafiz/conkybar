#!/bin/bash
killall conky
conky -c ~/.conky/conkybar/configs/conky_bg &
conky -p 1 -c ~/.conky/conkybar/configs/conky_txt &
conky -p 3 -c ~/.conky/conkybar/configs/conky_system &
conky -p 5 -c ~/.conky/conkybar/configs/conky_cpu &
conky -p 7 -c ~/.conky/conkybar/configs/conky_ram &
conky -p 9 -c ~/.conky/conkybar/configs/conky_files &
conky -p 11 -c ~/.conky/conkybar/configs/conky_network &
#conky -p 12 -c ~/.conky/conkybar/configs/conky_banshee &
conky -p 13 -c ~/.conky/conkybar/configs/conky_weather &
conky -p 15 -c ~/.conky/conkybar/configs/conky_clock 
exit
